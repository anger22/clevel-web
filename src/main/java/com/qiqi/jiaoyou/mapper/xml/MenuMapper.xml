<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiqi.jiaoyou.mapper.MenuMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.qiqi.jiaoyou.pojo.Menu">
                    <id column="f_id" property="fId"/>
                    <result column="f_clasid" property="fClasid"/>
                    <result column="f_tid" property="fTid"/>
                    <result column="f_oid" property="fOid"/>
                    <result column="f_name" property="fName"/>
                    <result column="f_weight" property="fWeight"/>
                    <result column="f_address" property="fAddress"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            f_id AS fId, f_clasid as fClasid, f_tid as fTid, f_oid as fOid, f_name as fName, f_weight as fWeight, f_address as fAddress
        </sql>

</mapper>