<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiqi.jiaoyou.mapper.DiamondsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qiqi.jiaoyou.pojo.Diamonds">
        <id column="diamonds_id" property="diamondsId" />
        <result column="diamonds_product_id" property="diamondsProductId" />
        <result column="diamonds_name" property="diamondsName" />
        <result column="diamonds_price" property="diamondsPrice" />
        <result column="diamonds_size" property="diamondsSize" />
        <result column="diamonds_type" property="diamondsType" />
        <result column="diamonds_terminal" property="diamondsTerminal" />
        <result column="month" property="month" />
        <result column="type" property="type" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
diamonds_name as diamondsName,month,type,
        diamonds_id AS diamondsId, diamonds_product_id AS diamondsProductId, diamonds_price AS diamondsPrice, diamonds_size AS diamondsSize, diamonds_type AS diamondsType, diamonds_terminal AS diamondsTerminal
    </sql>

</mapper>
