<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiqi.jiaoyou.mapper.CarMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qiqi.jiaoyou.pojo.Car">
        <id column="id" property="id" />
        <result column="memberId" property="memberId" />
        <result column="driving_license_photo" property="drivingLicensePhoto" />
        <result column="images" property="images" />
        <result column="vehicleType" property="vehicleType" />
        <result column="model" property="model" />
        <result column="addTime" property="addTime" />
        <result column="weight" property="weight" />
        <result column="deledeState" property="deledeState" />
        <result column="auditState" property="auditState" />
        <result column="reason" property="reason" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, memberId, driving_license_photo as drivingLicensePhoto, images, vehicleType, model,addTime, weight, deledeState,auditState,reason
    </sql>

</mapper>