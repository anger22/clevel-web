<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiqi.jiaoyou.mapper.LevelMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.qiqi.jiaoyou.pojo.Level">
                    <id column="id" property="id"/>
                    <result column="levelName" property="levelName"/>
                    <result column="empiricalValue" property="empiricalValue"/>
                    <result column="addTime" property="addTime"/>
                    <result column="editTime" property="editTime"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, levelName, empiricalValue, addTime, editTime
        </sql>

</mapper>